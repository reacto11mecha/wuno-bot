{
  "name": "wuno-bot",
  "version": "1.0.0",
  "description": "Bot whatsapp yang berguna untuk bermain UNO",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "nodemon src/index.ts",
    "start": "node .",
    "build": "tsc",
    "build:doc": "typedoc",
    "build:all": "tsc && typedoc",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier -w ./src ./test",
    "format:check": "prettier --check ./src ./test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "lint": "eslint ./src ./test",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reacto11mecha/wuno-bot.git"
  },
  "author": "Ezra Khairan Permana",
  "license": "MIT",
  "keywords": [
    "typescript",
    "uno",
    "whatsapp-bot"
  ],
  "bugs": {
    "url": "https://github.com/reacto11mecha/wuno-bot/issues"
  },
  "homepage": "https://github.com/reacto11mecha/wuno-bot#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.3.1",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.16.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.16.0",
    "@t3-oss/env-core": "^0.6.0",
    "dotenv": "^16.3.1",
    "libphonenumber-js": "^1.10.36",
    "luxon": "^3.3.0",
    "nanoid": "3.x.x",
    "node-cache": "^5.1.2",
    "p-limit": "3.1.0",
    "p-queue": "6.x.x",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.32.1",
    "whatsapp-web.js": "^1.21.0",
    "zod": "^3.21.4"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
